<script>
    $(document).ready(function () {

        $('.table').DataTable(
            {
                "iDisplayLength": 10,
                dom: 'Bfrtip',
                buttons: [
                    'copy', 'csv', 'excel', 'pdf', 'print'
                ]
            });
        $('.tabledisplay').DataTable(
            {
                "iDisplayLength": 100,
                "ordering": false,
                dom: 'Bfrtip',
                buttons: [
                    'copy', 'csv', 'excel', 'pdf', 'print'
                ]
            });
    });
</script>


